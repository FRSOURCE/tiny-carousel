{
  "name": "frs-table-calendar",
  "version": "0.1.1",
  "description": "Calendar extension for FRSolutions/FRS-table.",
  "main": "index.js",
  "scripts": {
    "test": "tape src/*.spec.js",
    "test-coverage": "yarn test --cov --coverage-report=lcov",
    "prestart": "yarn clean",
    "start": "parcel watch",
    "prebuild": "yarn clean",
    "build": "parcel build index.js -o FRS-table --detailed-report && parcel build docs/example.src.html -d docs -o example.dist.html",
    "postbuild": "yarn docs",
    "prepublishOnly": "yarn test-coverage && yarn build",
    "postpublish": "git add dist && git push origin --all && git push origin --tags",
    "predevExample": "yarn clean",
    "devExample": "parcel docs/example.src.html -d docs -o example.dist.html",
    "clean": "rimraf dist/**/* && rimraf docs/*.dist.html && rimraf docs/*.{js,css,map} && rimraf report.html",
    "docs": "jsdoc2md src/**/*.js > docs/api.md"
  },
  "author": "Jakub Freisler <FRSgit@users.noreply.github.com>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/FRSource/FRS-table-calendar#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FRSource/FRS-table-calendar.git"
  },
  "keywords": [
    "carousel",
    "css",
    "js",
    "efficient",
    "accessibility",
    "lightweight",
    "snap points",
    "CSS snap",
    "snapping points",
    "simple",
    "vanillaJS"
  ],
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "yarn.lock"
  ],
  "bugs": {
    "url": "https://github.com/FRSource/FRS-tiny-carousel/issues"
  },
  "devDependencies": {
    "axe-core": "^3.0.2",
    "jsdoc-to-markdown": "^4.0.1",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.10.0",
    "parcel-plugin-bundle-visualiser": "^1.0.2",
    "postcss-import": "^11.1.0",
    "postcss-reporter": "^5.0.0",
    "precss": "^3.1.2",
    "puppeteer": "^1.3.0",
    "rimraf": "^2.6.2",
    "tape": "^4.9.0"
  },
  "dependencies": {}
}
