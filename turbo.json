{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "outputs": []
    },
    "coverage": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "fix:lint": {
      "outputs": []
    },
    "watch:test": {
      "cache": false,
      "outputs": []
    },
    "start": {
      "cache": false
    }
  }
}